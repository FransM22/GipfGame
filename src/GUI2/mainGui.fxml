<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.SwingNode?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
         prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.40"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI2.Controller">
    <tabs>
        <Tab text="Game">
            <content>
                <VBox>
                    <children>
                        <SwingNode fx:id="gipfGameNode"/>
                    </children>
                </VBox>
            </content>
        </Tab>
        <Tab text="Analyze game">
            <content>
                <SplitPane dividerPositions="0.75">
                    <items>
                        <TreeTableView fx:id="boardStateTreeTableView">
                            <columns>
                                <TreeTableColumn fx:id="columnBoardName" prefWidth="150.0" text="Board ID"/>
                                <TreeTableColumn fx:id="columnWhiteReserve" prefWidth="75.0" text="White Reserve"/>
                                <TreeTableColumn fx:id="columnBlackReserve" prefWidth="75.0" text="Black Reserve"/>
                                <TreeTableColumn fx:id="columnWhiteGipf" prefWidth="32.5" text="WG"/>
                                <TreeTableColumn fx:id="columnBlackGipf" prefWidth="32.5" text="BG"/>
                                <TreeTableColumn fx:id="columnCurrentPlayer" prefWidth="75.0" text="Current player"/>
                                <TreeTableColumn fx:id="columnHeuristic0" prefWidth="75.0" text="Heuristic #0"/>
                            </columns>
                        </TreeTableView>
                        <VBox>
                            <children>
                                <SwingNode fx:id="smallGipfGameVisualisationNode"/>
                                <TitledPane animated="false" collapsible="false" text="White player">
                                    <content>
                                        <Label fx:id="whitePlayerDescriptionLabel" text="">
                                            <font>
                                                <Font name="Monospaced Regular" size="11.0"/>
                                            </font>
                                        </Label>
                                    </content>
                                </TitledPane>
                                <TitledPane animated="false" collapsible="false" text="Black player">
                                    <content>
                                        <Label fx:id="blackPlayerDescriptionLabel" text="">
                                            <font>
                                                <Font name="Monospaced Regular" size="11.0"/>
                                            </font>
                                        </Label>
                                    </content>
                                </TitledPane>
                            </children>
                        </VBox>
                    </items>
                </SplitPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
